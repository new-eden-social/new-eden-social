{
  "extends": [
    "tslint-angular"
  ],
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "max-classes-per-file": [
      false
    ]
  }
}