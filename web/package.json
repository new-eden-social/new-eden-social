  "license": "GPL-3.0",
  "engines": {
    "node": "^8.9.4"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "exit 0 || ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "localization": "ng xi18n --output-path locale",
    "heroku-postbuild": "npm run prebuild && ng build",
    "prebuild": "eval \"echo \"$(cat src/environments/environment.ts.template)\"\" > src/environments/environment.ts",
    "prebuild:production": "eval \"echo \"$(cat src/environments/environment.ts.template)\"\" > src/environments/environment.ts",
    "build:client-and-server-bundles": "npm run prebuild:production && ng build --prod && ng run eve-book-web:server:production",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@ctrl/ngx-emoji-mart": "^0.4.0",
